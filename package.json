{
  "name": "richtypo",
  "version": "1.0.1",
  "description": "Typography enhancer for English and Russian",
  "homepage": "https://github.com/sapegin/richtypo.js",
  "author": {
    "name": "Artem Sapegin",
    "url": "http://sapegin.me/"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/sapegin/richtypo.js.git"
  },
  "bugs": {
    "url": "https://github.com/sapegin/richtypo.js/issues"
  },
  "license": "MIT",
  "main": "./richtypo.js",
  "bin": {
    "richtypo": "./bin/cli.js"
  },
  "files": [
    "richtypo.js",
    "bin",
    "rules",
    "example/example.js"
  ],
  "engines": {
    "node": ">= 0.10.0"
  },
  "dependencies": {
    "lodash": "~4.6.1",
    "optimist": "~0.6.1"
  },
  "devDependencies": {
    "eslint": "^4.1.1",
    "eslint-config-tamia": "^4.1.1",
    "eslint-plugin-prettier": "^2.1.2",
    "fledermaus": "~4.0.0",
    "gh-pages": "~0.8.0",
    "jest": "^20.0.4",
    "prettier": "^1.5.2"
  },
  "scripts": {
    "pretest": "npm run lint",
    "test": "npm run test:jest",
    "test:jest": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --cache --fix",
    "postpublish": "npm run example && npm run gh-pages",
    "example": "node example/src",
    "gh-pages": "gh-pages -d example"
