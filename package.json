{
  "name": "richtypo",
  "version": "0.0.0-development",
  "description": "Typography enhancer for English and Russian",
  "homepage": "https://github.com/sapegin/richtypo.js",
  "author": {
    "name": "Artem Sapegin",
    "url": "http://sapegin.me/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sapegin/richtypo.js.git"
  },
  "bugs": {
    "url": "https://github.com/sapegin/richtypo.js/issues"
  },
  "license": "MIT",
  "main": "./richtypo.js",
  "files": [
    "richtypo.js",
    "rules"
  ],
  "engines": {
    "node": ">=4"
  },
  "dependencies": {},
  "devDependencies": {
    "eslint": "^4.1.1",
    "eslint-config-tamia": "^4.1.1",
    "eslint-plugin-prettier": "^2.1.2",
    "fledermaus": "^4.0.0",
    "gh-pages": "^1.0.0",
    "jest": "^20.0.4",
    "prettier": "^1.5.2",
    "semantic-release": "^6.3.6",
    "semantic-release-tamia": "^1.1.1"
  },
  "scripts": {
    "pretest": "npm run lint",
    "test": "npm run test:jest",
    "test:jest": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --cache --fix",
    "postpublish": "npm run example && npm run gh-pages",
    "example": "node example/src",
    "gh-pages": "gh-pages -d example",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "keywords": [
    "typography",
    "typo",
    "russian",
    "english"
  ],
  "tonicExampleFilename": "example/example.js",
  "release": {
    "analyzeCommits": "semantic-release-tamia/analyzeCommits",
    "generateNotes": "semantic-release-tamia/generateNotes",
    "verifyRelease": "semantic-release-tamia/verifyRelease"
  }
}
